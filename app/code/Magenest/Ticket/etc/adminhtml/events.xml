<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="catalog_product_save_after">
        <observer name="event_ticket" instance="Magenest\Ticket\Observer\Backend\EventTicketObserver"/>
    </event>
    <event name="sales_order_item_save_commit_after">
        <observer name="generate_ticket_backend" instance="Magenest\Ticket\Observer\GenerateTicket"/>
    </event>
    <event name="sales_order_item_cancel">
        <observer name="ticket_cancel_order" instance="Magenest\Ticket\Observer\Backend\CancelOrder"/>
    </event>
</config>