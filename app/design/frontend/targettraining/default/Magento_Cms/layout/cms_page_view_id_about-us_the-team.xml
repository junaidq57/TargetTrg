<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breadcrumbs" remove="true" />
        <referenceContainer name="header.container">
            <block class="Magento\Cms\Block\Block" name="banner-team-page">
                <arguments>
                    <argument name="block_id" xsi:type="string">banner_team_page</argument>
                </arguments>
            </block>
        </referenceContainer>
        <referenceBlock name="top.event.finder">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">TargetTraining_EventFinder::widget/home-event-finder.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>